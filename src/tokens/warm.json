{
  "color": {
    "$type": "color",
    "primary": {
      "$value": "#007aff"
    },
    "secondary": {
      "$value": "#6c6c6c"
    },
    "baseline": {
      "$value": "#1292a1"
    },
    "onBaseline": {
      "$value": "#ffffff"
    }
  },
  "typography": {
    "$type": "typography",
    "display": {
      "lg": {
        "$value": {
          "fontSize": "4rem",
          "fontWeight": 700,
          "fontFamily": ["Russo One", "sans-serif"]
        }
      }
    }
  },
  "elevation": {
    "$type": "elevation",
    "surface": {
      "sunken": {
        "$value": "-0.09"
      },
      "baseline": {
        "$value": "0"
      },
      "raised": {
        "$value": "0.1"
      },
      "overlay": {
        "$value": "100%"
      }
    },
    "shadow": {
      "sunken": {
        "$value": "-0.2"
      },
      "baseline": {
        "$value": "0"
      },
      "raised": {
        "$value": "-0.05"
      },
      "overlay": {
        "$value": "-0.08"
      }
    }
  }
}
